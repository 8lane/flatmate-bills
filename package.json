    "babel-jest": "^22.2.2",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-jest": "^22.2.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
